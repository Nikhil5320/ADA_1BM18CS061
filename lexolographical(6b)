#include<iostream>
#include<array>
using namespace std;

int i,j,n,indeg[20],bulb[20],count=0,a[20][20],temp[20]={0},t,m,c=0;

int lexico()
{
    if(c>=n)
     return 0;
    m=temp[0];
    for(j=1;j<count;j++)
    {
         if(temp[j]<m)
           m=temp[j];
    }
    if(m!=0)
    {
      cout<<m<<"\t";
      c++;
      bulb[m]=1;
      for(j=1;j<=n;j++)
      {
         if(a[m][j]==1)
            indeg[j]--;
      }
    }
    t=0;
    count=0;
       for(i=1;i<=n;i++)
       {
           if(bulb[i]==0 && indeg[i]==0)
           {
              temp[t++]=i;
              count++;
           }
       }
       lexico();
}

int main()
{
    cout<<"Enter the number of vertices : "<<endl;
    cin>>n;

    cout<<"Enter graph data in matrix form : "<<endl;
    for(i=1; i<=n; i++)
     for(j=1;j<=n;j++)
       cin>>a[i][j];
    for(i=1;i<=n;i++)
    {
        indeg[i]=0;
        bulb[i]=0;
    }
    for(j=1;j<=n;j++)
      for(i=1;i<=n;i++)
        indeg[j]=indeg[j]+a[i][j];
    cout<<"\nThe lexicographical order is: "<<endl;
    lexico();
    
    return 0;
}
